---
import Container from "../components/Container.astro";
---

<header>
  <nav
    class="absolute z-10 w-full border-b border-black/5 dark:border-white/5 lg:border-transparent pt-12"
  >
    <Container>
      <div
        class="relative flex items-center justify-between gap-6 py-3 md:gap-0 md:py-4"
      >
        <div class="relative z-20 flex justify-between md:px-0 lg:w-max">
          <div aria-label="logo" class="flex items-center text-lg">
            <div aria-hidden="true" class="flex space-x-1 font-light">KY Senior</div>
            <span class="font-semibold text-blue-600">Health</span>
          </div>
        </div>

        <a href="#contact">
         
            <div class="flex gap-2 items-center justify-center rounded-md border-2 border-solid border-blue-600 hover:border-blue-500 bg-blue-600 text-white hover:text-slate-100 hover:bg-blue-500 active:bg-blue-800 active:text-blue-100 focus-visible:outline-blue-600 px-6 py-3 font-semibold hover:cursor-pointer" onclick="showAlert()">
              Let's chat
            </div>
          
      </a>
      </div>
    </Container>
  </nav>
</header>
