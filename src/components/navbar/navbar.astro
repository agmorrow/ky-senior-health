---
import Container from "@components/container.astro";
import Link from "@components/ui/link.astro";
import { Astronav } from "astro-navbar";

---

<Container>
  <header class="flex lg:flex-row justify-between items-center my-5">
    <Astronav>
      <div class="flex items-center justify-between">
        <a href="/" class="text-lg"
          ><span class="font-bold text-slate-800">KY Senior</span><span
            class="text-slate-500">Health</span
          >
        </a>
      </div>
    </Astronav>
    <div>
      <div class="flex items-center gap-4">
        <Link href="#" size="md">Let's chat</Link>
      </div>
    </div>
  </header>
</Container>
